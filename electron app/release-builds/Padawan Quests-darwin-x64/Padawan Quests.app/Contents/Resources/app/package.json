{
  "name": "padawan-quests",
  "productName": "Padawan Quests",
  "version": "1.0.0",
  "description": "Github issue finder for developers. ",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/512x512.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . padawan-quests --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/512x512.ico --prune=true --out=release-builds --version-string.CompanyName=Blck --version-string.FileDescription=1.0.0 --version-string.ProductName=\"Padawan Quests\"",    
    "package-linux": "electron-packager . padawan-quests --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/512x512.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IceDune/padawan-quests.git"
  },
  "keywords": [
    "issues",
    "github"
  ],
  "author": "IceDune",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/IceDune/padawan-quests/issues"
  },
  "homepage": "https://github.com/IceDune/padawan-quests#readme",
  "devDependencies": {
    "electron": "^3.0.6",
    "electron-packager": "^12.2.0"
  },
  "dependencies": {
    "spin.js": "^4.0.0"
  }
}
